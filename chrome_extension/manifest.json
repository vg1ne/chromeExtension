{
  "name": "ChangePageText",
  "description": "A browser action with a popup that changes the page text",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
  ],
  "browser_action": {
      "default_title": "Set text",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
